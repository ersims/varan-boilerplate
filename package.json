{
  "name": "varan-boilerplate",
  "private": true,
  "license": "MIT",
  "scripts": {
    "apply:format": "prettier --config .prettierrc --write \"{src,test,types}/**/*.{ts,js}?(x)\"",
    "build": "varan build",
    "check:format": "prettier --config .prettierrc --list-different \"{src,test,types}/**/*.{ts,js}?(x)\"",
    "check:linting": "tslint --project tsconfig.json \"{src,test,types}/**/*.{ts,js}?(x)\"",
    "check:types": "tsc --noEmit",
    "check": "npm-run-all check:*",
    "clean": "del-cli ./dist",
    "heroku-postbuild": "npm install --only=dev && npm run build && npm prune --production",
    "start": "node dist/server/bin/web.js",
    "test": "jest",
    "prewatch": "npm run clean",
    "watch": "varan watch -- --inspect"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "connect-gzip-static": "2.1.1",
    "connected-react-router": "4.3.0",
    "core-js": "2.5.7",
    "express": "4.16.3",
    "history": "4.7.2",
    "lodash": "4.17.10",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.3",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "recompose": "0.27.1",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-logger": "3.0.6",
    "redux-observable": "1.0.0",
    "reduxsauce": "1.0.0",
    "rxjs": "6.2.1",
    "serialize-javascript": "1.5.0",
    "source-map-support": "0.5.6",
    "typesafe-actions": "2.0.4",
    "varan": "0.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.53",
    "@types/babel-core": "6.25.5",
    "@types/classnames": "2.2.4",
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/express": "4.16.0",
    "@types/jest": "23.1.5",
    "@types/lodash": "4.14.111",
    "@types/node": "8.10.21",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-helmet": "5.0.6",
    "@types/react-redux": "6.0.4",
    "@types/react-router": "4.0.28",
    "@types/react-router-dom": "4.2.7",
    "@types/react-test-renderer": "16.0.1",
    "@types/recompose": "0.26.1",
    "@types/redux-logger": "3.0.6",
    "@types/serialize-javascript": "1.3.2",
    "@types/webpack-env": "1.13.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.0",
    "del-cli": "1.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "jest": "23.4.0",
    "normalize.css": "8.0.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.7",
    "react-test-renderer": "16.4.1",
    "supertest": "3.1.0",
    "ts-jest": "23.0.0",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "5.9.2",
    "tslint-config-prettier": "1.13.0",
    "tslint-eslint-rules": "5.3.1",
    "typescript": "2.9.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}?(x)",
      "*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 10,
        "statements": 5
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/test/__mocks__/fileMock.ts",
      "\\.(css|sass|scss|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/fixtures/setupTests.ts",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/__tests__/**/*.{ts,js}?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "silent": true,
    "verbose": true
  }
}
